    "      'Grocery', \n",
    "      2a70494e-6b68-4739-b3e0-ff06aa0a2d67, \n",
    "      true\n",
    "    ) \n",
    "    USING TTL 5;\n",
    "  \"  \n",
    "\n",
    "  ./bin/ycqlsh --execute \"\n",
    "    SELECT * FROM ks_ybu.tbl_wishlists_by_user;\n",
    "  \"  "
